{
    "name":"Demo Extension",
    "version" : "1.0.0",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches":["<all_urls>"],
            "js":["src/js/content.js"]
        }
    ],
    "browser_action": {
        "default_title": "Demo extension"
    },
    "background": {
        "scripts": ["src/js/background.js"]
    },
    "permissions": ["tabs"]
}